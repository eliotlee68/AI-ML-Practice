def mult_scalar(A, c):
    """
    Returns a new matrix created by multiplying elements of matrix A by a scalar c.
    """
    # TODO: add your solution here and remove `raise NotImplementedError`
    m = len(A)
    if (m == 0):
        n = 0
    else:
        n = len(A[0])
    B = []
    for i in range(m):
        temp_arr = []
        for j in range(n):
            temp_arr.append(A[i][j] * c)
        B.append(temp_arr)
    return B
  
